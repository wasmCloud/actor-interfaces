namespace "wasmcloud:httpclient"

interface {
  HandleRequest{request: Request}: Response
}

type Request {
  url: string
  method: string
  path: string
  queryString: string
  header: {string:string}
  body: bytes
}

type Response {
  statusCode: u32
  status: string
  header: {string:string}
  body: bytes
}
